<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:WpfApp3.Skins.Default">

  <system:String x:Key="Copyright" >Todo</system:String>
  <x:Static Member="local:Properties.ApplicationTitle" x:Key="ApplicationTitle"/>

  <BitmapImage x:Key="LogoLarge" UriSource="pack://application:,,,/skins/default/acme.png" CacheOption="None" />
  
  <Style x:Key="H1" TargetType="{x:Type ContentControl}">
        <Setter Property="FontFamily" Value="Arial"/>
        <Setter Property="FontSize" Value="24pt"/>
        <Setter Property="FontWeight" Value="Bold"/>
      </Style>
</ResourceDictionary>